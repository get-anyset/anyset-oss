{
  "name": "anyset-py-lib",
  "root": "anyset-py-lib",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd anyset-py-lib && uv build"
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd anyset-py-lib && uv publish --username __token__ --password $PYPI_TOKEN"
      }
    }
  }
} 
