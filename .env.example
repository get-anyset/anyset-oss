SNOWFLAKE_BASIC_SAMPLE_DATASET_USER = "{update-with-your-snowflake-user}"
SNOWFLAKE_BASIC_SAMPLE_DATASET_PASSWORD = "{update-with-the-snowflake-user-password}"

SNOWFLAKE_PK_SAMPLE_DATASET_USER = "{update-with-your-snowflake-user}"
SNOWFLAKE_PK_SAMPLE_DATASET_PRIVATE_KEY = "{update-with-the-snowflake-user-private-key}"
SNOWFLAKE_PK_SAMPLE_DATASET_PRIVATE_KEY_PASSPHRASE = "{update-with-the-private-key-passphrase}"
