APP_LOG_LEVEL = "DEBUG"
APP_DEBUG = true
APP_PORT = 9025

PG_POOL_MIN_SIZE = 1
PG_POOL_MAX_SIZE = 10
PG_QUERY_TIMEOUT = 30

PG_SAMPLE_DATASET_HOST = "localhost"
